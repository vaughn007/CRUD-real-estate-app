      <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarCollapse" aria-controls="navbarCollapse" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
      </button>
      <div class="collapse navbar-collapse" id="navbarCollapse">
        <ul class="navbar-nav mr-auto">
          <li class="nav-item active">
            <a class="nav-link" href="#"> <span><i class="fa fa-volume-control-phone" aria-hidden="true"></i></span>&nbsp;833-HOME-BY-E</a>
          </li>
          
        </ul> 
		  
		<ul class="navbar-nav">
			<li class="nav-item"><a class="nav-link" href="/homes/new"><span><i class="fa fa-home" aria-hidden="true"></i></span>&nbsp; Add home</a>
			</li>
                <% if(!currentUser) { %>
                <li class="nav-item"><a href="/login" class="nav-link"><span><i class="fa fa-sign-in"></i></span>&nbsp;Login</a></li>
                
                
                <% } else { %>
                <li class="nav-item"><a href="#" class="nav-link">Signed In As <%= currentUser.username %></a> </li>
                <li class="nav-item"><a href="/logout" class="nav-link">Logout</a></li>
                <% } %>
                
            </ul>
		  
      </div>
    </nav>

<div class="container">
        
        <% if(error && error.length > 0){ %>
            <div class="alert alert-danger" role="alert">
                <%= error %>
            </div>
        <% } %>
        <% if(success && success.length > 0){ %>
            <div class="alert alert-success" role="alert">
                <%= success %>
            </div>
        <% } %>
    </div>
    

   
